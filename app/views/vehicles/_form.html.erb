<%= form_with(model: [@shipping_company, @vehicle]) do |f| %>
  <div>
    <%= f.label :model%>
    <%= f.text_field :model %>
  </div>

  <div>
    <%= f.label :brand %>
    <%= f.text_field :brand %>
  </div>

  <div>
    <%= f.label :plate %>
    <%= f.text_field :plate %>
  </div>

  <div>
    <%= f.label :year_fabrication %>
    <%= f.text_field :year_fabrication %>
  </div>

  <div>
    <%= f.label :freight %>
    <%= f.text_field :freight %>
  </div>

  <div>
    <%= f.submit 'Enviar' %>
  </div>
<% end %>