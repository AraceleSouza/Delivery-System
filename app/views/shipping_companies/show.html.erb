<% if functionary_signed_in? %>
  <div>
    <%= link_to('Veículos', shipping_company_vehicles_path(@shipping_company.id))%>| 
    <%= link_to('Tabela de Preços', shipping_company_price_settings_path(@shipping_company.id)) %>|
    <%= link_to('Prazos de Entrega', shipping_company_deadlines_path(@shipping_company.id))%>|
  </div>
<% end %>

<%= link_to('Produtos', shipping_company_product_models_path(@shipping_company.id))%>

<h2>Detalhes da transportadora: </h2>
  <div>
    <strong><%= ShippingCompany.human_attribute_name(:fantasy_name)%>:</strong> <%= @shipping_company.fantasy_name %><br>
    <strong><%= ShippingCompany.human_attribute_name(:corporate_name)%>:</strong> <%= @shipping_company.corporate_name %><br>
    <strong><%= ShippingCompany.human_attribute_name(:cnpj)%>:</strong> <%= @shipping_company.cnpj %><br>
    <strong><%= ShippingCompany.human_attribute_name(:email)%>:</strong> <%= @shipping_company.email %><br>
    <strong><%= ShippingCompany.human_attribute_name(:address)%>:</strong> <%= @shipping_company.address %><br>
    <strong><%= ShippingCompany.human_attribute_name(:city)%>:</strong> <%= @shipping_company.city %><br>
    <strong><%= ShippingCompany.human_attribute_name(:state)%>:</strong> <%= @shipping_company.state %><br>
    <strong><%= ShippingCompany.human_attribute_name(:cep)%>:</strong> <%= @shipping_company.cep %>
  </div>

<% if admin_signed_in? %>
  <div>
    <%= link_to 'Voltar', shipping_companies_path %> | <%= link_to('Editar', edit_shipping_company_path(@shipping_company.id))%>
  </div>
<% end %>



