<%= form_with(model: @shipping_company) do |f| %>
  <div>
    <%= f.label :corporate_name, 'Razão Social'%>
    <%= f.text_field :corporate_name %>
  </div>

  <div>
    <%= f.label :fantasy_name, 'Nome Fantasia' %>
    <%= f.text_field :fantasy_name%>
  </div>

  <div>
    <%= f.label :cnpj, 'CNPJ'%>
    <%= f.text_field :cnpj %>
  </div>

  <div>
    <%= f.label :email, 'E-mail' %>
    <%= f.text_field :email %>
  </div>

  <div>
    <%= f.label :address, 'Endereço' %>
    <%= f.text_field :address %>
  </div>

  <div>
    <%= f.label :city, 'Cidade:' %>
    <%= f.text_field :city %>
  </div>

  <div>
    <%= f.label :state, 'Estado' %>
    <%= f.text_field :state %>
  </div>

  <div>
    <%= f.label :cep, 'CEP' %>
    <%= f.number_field :cep %>
  </div>

  <div>
    <%= f.submit 'Enviar' %>
  </div>
<% end %>